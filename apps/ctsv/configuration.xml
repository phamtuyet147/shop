<?xml version="1.0" encoding="utf-8" ?>
<appSetting xmlns="http://linhnv.xyz/app.configuration">

    <developer>true</developer>
    <defaultLanguage>vi_VN</defaultLanguage>

    <connection>
        <hostname>localhost</hostname>
        <databaseName>ctsv</databaseName>
        <username>ctsv</username>
        <password>123456</password>
        <hashType>md5</hashType>
    </connection>

    <directory>
        <resources>resources</resources>
        <properties>properties</properties>
        <language>resources/lang</language>
        <cacheView>cache</cacheView>
        <sql>resources/sql</sql>
    </directory>

    <hash>
        <prefix>ctsv</prefix>
        <defaultHashType>md5</defaultHashType>
    </hash>

</appSetting>